                            border-width: 0px;
                            border-radius: 50%;
                            background-color: #1da1f2;
                            height: 120px;
                            width: 120px;
                            font-family: 'Artifakt Element';
                            font-weight: 400;
                            font-style: normal;
                            font-size: 14px;
                            text-align: center;
                            line-height: normal;
                        }

